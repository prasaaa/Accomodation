<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_INSERT_Controller</servlet-name>
        <servlet-class>com.Controller.User_INSERT_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Property_INSERT_Controller</servlet-name>
        <servlet-class>com.Controller.Property_INSERT_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_Property</servlet-name>
        <servlet-class>com.Controller.Delete_Property</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_Property</servlet-name>
        <servlet-class>com.Controller.Update_Property</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Property_Update_Controller</servlet-name>
        <servlet-class>com.Controller.Property_Update_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_Owner</servlet-name>
        <servlet-class>com.Controller.Delete_Owner</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_Properties</servlet-name>
        <servlet-class>com.Controller.Delete_Properties</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login_Controller</servlet-name>
        <servlet-class>com.Controller.login_Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>servlet.SignupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User_INSERT_Controller</servlet-name>
        <url-pattern>/User_INSERT_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Property_INSERT_Controller</servlet-name>
        <url-pattern>/Property_INSERT_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_Property</servlet-name>
        <url-pattern>/Delete_Property</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_Property</servlet-name>
        <url-pattern>/Update_Property</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Property_Update_Controller</servlet-name>
        <url-pattern>/Property_Update_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_Owner</servlet-name>
        <url-pattern>/Delete_Owner</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_Properties</servlet-name>
        <url-pattern>/Delete_Properties</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_Controller</servlet-name>
        <url-pattern>/login_Controller</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>main_home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
